generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Profile {
  id        String   @id @default(cuid())
  slug      String   @unique
  email     String   @unique
  name      String
  data      Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
